<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="FieldView" tools:ignore="ResourceName">
        <attr name="comm_labelTextSize" format="dimension"/>
        <attr name="comm_labelWidth" format="dimension"/>
        <attr name="comm_labelTextColor" format="reference"/>
        <attr name="comm_labelBgColor" format="reference"/>
        <attr name="comm_dataBgColor" format="reference"/>
        <attr name="comm_label_name" format="string"/>
        <attr name="comm_dict_key" format="string"/>
        <attr name="comm_dict_default_name" format="string"/>
        <attr name="comm_dict_default_value" format="string"/>
        <attr name="comm_line_half_show" format="boolean"/>
        <attr name="comm_field_index" format="integer"/>
        <attr name="comm_field_primary_index" format="integer"/>
        <attr name="comm_edittext_hint" format="string"/>
        <attr name="comm_edittext_line" format="integer"/>
        <attr name="comm_must" format="boolean"/>
        <attr name="comm_must_for_mutiview" format="string"/>
        <attr name="comm_showMustInput" format="boolean"/>
        <attr name="comm_field_enable" format="boolean"/>
        <attr name="comm_field_name" format="string"/>
        <attr name="comm_init_content" format="string"/>
        <attr name="comm_warnMessager" format="string"/>
        <attr name="comm_textIcon" format="reference"/>
        <attr name="comm_contentType" format="enum">
            <enum name="TYPE_EDITTEXT" value="1"/>
            <enum name="TYPE_TEXTVIEW" value="2"/>
            <enum name="TYPE_RADIOGROUP" value="3"/>
            <enum name="TYPE_SPINNER" value="4"/>
            <enum name="TYPE_HIDDENVIEW" value="5"/>
            <enum name="TYPE_MUTIVIEW" value="6"/>
            <enum name="TYPE_CHECKBOX" value="7"/>
        </attr>
        <attr name="comm_labelWithMustIcon" format="boolean"/>
        <attr name="comm_fieldViewBgColor" format="reference"/>
        <attr name="comm_valueTextSize" format="dimension"/>
        <attr name="comm_valueBgColor" format="reference"/>
        <attr name="comm_valueTextColor" format="reference"/>
        <attr name="comm_dividedColor" format="reference"/>
        <attr name="comm_fieldDivided" format="boolean"/>
        <attr name="comm_formDivided" format="boolean"/>
        <attr name="comm_formHorizontal" format="boolean"/>
        <attr name="comm_forceIgnore" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="SuperImageView" tools:ignore="ResourceName">
        <attr name="sImgFrameWidth" format="dimension"/>
        <attr name="sImgFrameColor" format="color"/>
        <attr name="sImgRatio" format="float"/>
        <attr name="sImgsupportCache" format="boolean"/>
        <attr name="sImgSupporCrop" format="boolean"/>
        <attr name="sImgConverType" format="enum">
            <enum name="FACE" value="1"/>
            <enum name="RECTANGLE" value="2"/>
            <enum name="SMART_FACE" value="3"/>
        </attr>
        <attr name="sImgMediaQuality" format="enum">
            <enum name="MEDIA_QUALITY_HIGHEST" value="14"/>
            <enum name="MEDIA_QUALITY_HIGH" value="13"/>
            <enum name="MEDIA_QUALITY_MEDIUM" value="12"/>
            <enum name="MEDIA_QUALITY_LOW" value="11"/>
            <enum name="MEDIA_QUALITY_LOWEST" value="15"/>
        </attr>
        <attr name="sImgCropX" format="integer"/>
        <attr name="sImgRequestCode" format="integer"/>
        <attr name="sImgCropY" format="integer"/>
        <attr name="sImgLookMode" format="boolean"/>
        <attr name="sImgSupportBigImage" format="boolean"/>
        <attr name="sImgOnlyCamera" format="boolean"/>
        <attr name="sImgImageHolder" format="reference"/>
        <attr name="sImgShape" format="enum">
            <enum name="rectangle" value="1"/>
            <enum name="roundness" value="2"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="KeyValueView" tools:ignore="ResourceName">
        <attr name="comm_key_text" format="string"></attr>
        <attr name="comm_key_size" format="dimension"></attr>
        <attr name="comm_key_color" format="reference"></attr>
        <attr name="comm_value_text" format="string"></attr>
        <attr name="comm_value_size" format="dimension"></attr>
        <attr name="comm_value_color" format="reference"></attr>
        <attr name="comm_kv_gravity" format="enum">
            <enum name="LEFT" value="1"></enum>
            <enum name="CENTER" value="2"></enum>
            <enum name="RIGHT" value="3"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="FieldSpinner" tools:ignore="ResourceName">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
    </declare-styleable>
    <declare-styleable name="AutoCompleteEditText" tools:ignore="ResourceName">
        <attr name="dismissWhenTouchOutside" format="boolean"/>
        <attr name="dictMode" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="SuperSearchView" tools:ignore="ResourceName">
        <attr name="defaultOpen" format="boolean"/>
        <attr name="android:hint"/>
        <attr name="android:textColor"/>
        <attr name="android:textColorHint"/>
        <attr name="android:inputType"/>
        <attr name="defaultRequestFocues" format="boolean"/>
        <attr name="supportCancel" format="boolean"/>
        <!--searchview 的背景，默认是白色，和toolbat使用的时候要设置成透明-->
        <attr name="searchViewBackground" format="integer"/>
        <!--topbar的背景颜色，默认是透明的，和toolbar使用是要设置成dark颜色-->
        <attr name="searchTopbarBackground" format="integer"/>
        <!--编辑框的背景颜色，默认是半透明-->
        <attr name="searchEditViewBackground" format="integer"/>
        <attr name="searchActionViewBackground" format="integer"/>
    </declare-styleable>
</resources>